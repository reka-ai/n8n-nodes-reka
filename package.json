{
	"name": "@reka-ai/n8n-nodes-reka",
	"version": "0.4.4-beta.2",
	"description": "Official n8n nodes to use Reka's AI in our workflows",
	"license": "MIT",
	"homepage": "https://reka.ai",
	"keywords": [
		"n8n-community-node-package",
		"reka",
		"n8n",
		"ai",
		"vision",
		"clipping",
		"video clipping",
		"vision ai",
		"reka ai",
		"video analysis",
		"image analysis"
	],
	"author": {
		"name": "Frank Boucher",
		"email": "frank@reka.ai"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/reka-ai/n8n-nodes-reka.git"
	},
	"scripts": {
		"build": "n8n-node build",
		"build:watch": "tsc --watch",
		"dev": "n8n-node dev",
		"lint": "n8n-node lint",
		"lint:fix": "n8n-node lint --fix",
		"release": "n8n-node release",
		"release:beta": "release-it --preRelease=beta --npm.tag=beta",
		"prepublishOnly": "npm run build"
	},
	"files": [
		"dist"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"strict": true,
		"credentials": [
			"dist/credentials/RekaApi.credentials.js"
		],
		"nodes": [
			"dist/nodes/Reka/Vision/RekaVision.node.js"
		]
	},
	"devDependencies": {
		"@n8n/node-cli": "*",
		"eslint": "9.32.0",
		"prettier": "3.6.2",
		"release-it": "^19.0.4",
		"typescript": "5.9.2"
	},
	"peerDependencies": {
		"n8n-workflow": "*"
	},
	"publishConfig": {
		"access": "public"
	}
}